TARGET_LIB = soil.a
OBJS = soil/image_DXT.o soil/image_helper.o soil/SOIL.o soil/stb_image_aug.o
	
INCDIR = ./
CFLAGS = -O2 -G0 -Wall
CXXFLAGS = $(CFLAGS)
ASFLAGS = $(CFLAGS)

LIBS = -lz ./lib/libGL.a ./lib/libGLU.a ./lib/libglut.a
LIBDIR = 
LDFLAGS =

PSPSDK=$(shell psp-config --pspsdk-path)
include $(PSPSDK)/lib/build.mak

install : $(TARGET_LIB)
	@cp soil.a lib/
	@echo "Done"
